doctype html
html
    head
        title #{title}
        link(type="image/x-icon", href="images/favico.png", rel="icon")
        meta(name="apple-mobile-web-app-capable", content="yes")
        script(src="/socket.io/socket.io.js")
        != js("basejs")
        != js("appjs")
        != css("appcss")
    body
        #wrapper
            include header
            include map
            include entityManager
            include variableManager
            include auditDataManager
            include auditEventManager
            include settings
            include help
            include start
            include scriptEditor
            include alerts
            include footer
        #loading
            object(data="images/loading.svg")
            div #{title} #{version} is loading, please wait...

        include tutorial